{
  "name": "grow5x-e2e-tests",
  "version": "1.0.0",
  "description": "Pruebas E2E funcionales completas para Grow5x",
  "main": "e2e-functional-tests.js",
  "scripts": {
    "test:e2e": "node e2e-functional-tests.js",
    "test:e2e:headless": "HEADLESS=true node e2e-functional-tests.js",
    "install:playwright": "npx playwright install",
    "setup": "npm install && npm run install:playwright",
    "env:check-mongo": "node test-mongodb-atlas-connection.js",
    "start:full": "concurrently \"cd backend && npm start\" \"cd frontend && npm run dev\""
  },
  "dependencies": {
    "axios": "^1.11.0",
    "colors": "^1.4.0",
    "concurrently": "^8.2.2",
    "dotenv": "^16.3.1",
    "mongoose": "^8.17.1",
    "node-fetch": "^3.3.2",
    "playwright": "^1.40.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0"
  },
  "keywords": [
    "e2e",
    "testing",
    "playwright",
    "grow5x",
    "functional-tests"
  ],
  "author": "Grow5x Team",
  "license": "MIT"
}
